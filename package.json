{
  "name": "gromozeka-bot",
  "version": "0.1.2",
  "description": "Telegram bot",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "gromozeka": "entry.js"
  },
  "scripts": {
    "start": "node dist/entry.js",
    "start:watch": "nodemon src/entry.ts --watch src --watch ../telebuilder --ext ts,json --delay 1",
    "lint": "eslint .",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "postbuild": "cp -r package.json config dist",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "jest",
    "postversion": "cp -r package.json ..",
    "prepublishOnly": "npm pkg delete scripts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/en9inerd/gromozeka-bot.git"
  },
  "keywords": [
    "telegram-api",
    "telegram",
    "user-management",
    "chat-management",
    "bot"
  ],
  "author": "Vladimir Loskutov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/en9inerd/gromozeka-bot/issues"
  },
  "homepage": "https://github.com/en9inerd/gromozeka-bot#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "config": "^3.3.9",
    "dotenv": "^16.3.1",
    "mongodb": "^5.6.0",
    "telebuilder": "^0.3.0",
    "telegram": "^2.17.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "eslint": "^8.44.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "semantic-release": "^21.0.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
